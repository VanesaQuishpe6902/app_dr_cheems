<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".soc_red_social">

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp"
        android:padding="20dp">

        <TextView
            android:id="@+id/textView59"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Red social - comentarios"
            android:textSize="34sp" />

        <Button
            android:id="@+id/btn_RespondidoRedSocial"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="respondido" />

        <Button
            android:id="@+id/btn_SinResponderRedSocial"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Sin responder" />

        <Button
            android:id="@+id/btn_TodosRedSocial"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Todos" />

        <ListView
            android:layout_width="match_parent"
            android:layout_height="343dp" />

        <Button
            android:id="@+id/bnt_VolverRedSocial"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="VOLVER" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>